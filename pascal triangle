class Solution:
    def getRow(self, rowIndex: int) -> List[int]:
        result = [] 
        ans = 1
        result.append(ans) 
        row = rowIndex+1 
        for col in range(1,rowIndex+1):
            ans = ans*(row-col) 
            ans = ans//(col) 
            result.append(ans) 
        return result 
